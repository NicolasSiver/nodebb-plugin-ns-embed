{
  "rules": [
    {
      "name": "youtube",
      "displayName": "Youtube",
      "icon": "fa-youtube",
      "regex": "(?:https?:\\/\\/)?(?:youtube\\.com\\/\\S*(?:(?:\\/e(?:mbed))?\\/|watch\\?(?:\\S*?&?v\\=))|youtu\\.be\\/)([a-zA-Z0-9_-]{6,11})",
      "replacement": "<div class='embedWrapper'><div class='embedContainer'><iframe src='https://www.youtube.com/embed/$1' frameborder='0' allowfullscreen></iframe></div></div>"
    },
    {
      "name": "vimeo",
      "displayName": "Vimeo",
      "icon": "fa-vimeo",
      "regex": "(?:https?:\\/\\/)?(?:www\\.)?vimeo\\.com\\/(?:channels\\/(?:\\w+\\/)?|groups\\/(?:[^\\/]*)\\/videos\\/|)(\\d+)(?:|\\/\\?)",
      "replacement": "<div class='embedWrapper'><div class='embedContainer'><iframe src='https://player.vimeo.com/video/$1' frameborder='0' allowfullscreen></iframe></div></div>"
    },
    {
      "name": "vine",
      "displayName": "Vine",
      "icon": "fa-vine",
      "regex": "(?:https?:\\/\\/)?(?:www\\.)?vine\\.co\\/v\\/([a-zA-Z0-9_-]{6,11})",
      "replacement": "<div class='embedWrapper'><div class='embedContainer'><iframe src='https://vine.co/v/$1/embed/simple' frameborder='0'></iframe></div></div>"
    }
  ]
}
